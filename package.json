{
  "name": "maqraaty_2-server",
  "version": "1.0.0",
  "description": "Hello World!",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon app.js",
    "electron": "electron-forge start",
    "winpackage": "electron-packager . مقرأتي --platform=win32 --arch=x64 --overwrite --app-version=1.0.0 --build-version=1.0.0 --win32metadata.ProductName=مقرأتي --FileDescription=مقرأتي --appname=مقرأتي --win32metadata.CompanyName=ng --app-copyright=ng --icon=dist/maqraaty_2/browser/favicon.ico",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "pkg": "electron-packager . مقرأتي --platform=win32 --arch=x64"
  },
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "icon": "/dist/maqraaty_2/browser/favicon.ico"
      },
      "packagerConfig": {
        "icon": "/dist/maqraaty_2/browser/favicon.ico"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-s3",
          "platforms": [
            "darwin",
            "linux"
          ],
          "config": {
            "bucket": "my-bucket",
            "folder": "my/key/prefix"
          }
        },
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Ahmed21kh",
              "name": "electron"
            },
            "prerelease": false,
            "draft": true
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "keywords": [],
  "author": "Ahmed Khaled",
  "license": "ISC",
