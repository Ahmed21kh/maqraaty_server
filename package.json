    "electron-squirrel-startup": "^1.0.0",
    "electron-wix-msi": "^5.1.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "livereload": "^0.9.3",
    "maqraaty_2-server": "file:",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "mongoose-sequence": "^6.0.1",
    "multer": "^1.4.5-lts.1",
